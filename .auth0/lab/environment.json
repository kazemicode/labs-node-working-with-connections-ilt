{
  "name": "Lab Node Working with Connections",
  "version": "1.1.2",
  "resources": "resources.yml",
  "postConfigureCommand": "test.sh",
  "unauthenticatedTour": ".auth0/lab/guides/signin.tour",
  "postConfigureTour": ".auth0/lab/guides/working-with-connections.tour",
  "clients": [
    {
      "name": "Lab: Web App",
      "directory": "src/web-app",
      "env": {
        "ISSUER_BASE_URL": "$tenant_base_url",
        "CLIENT_ID": "$client_id",
        "CLIENT_SECRET": "$client_secret",
        "SESSION_SECRET": "a long, randomly-generated string stored in env",
        "PORT": 37500
      }
    },
    {
      "name": "Lab: Web App2",
      "directory": "src/web-app",
      "env": {
        "IMP_CLIENT_ID":"$client_id",
        "IMP_CLIENT_SECRET":"$client_secret",
        "IMP_SESSION_SECRET": "a long, randomly-generated string stored in env",
        "IMP_PORT": 38500
      }
    }
  ],
  "resourceServers": []
}
